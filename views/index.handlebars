<div class="jumbotron">
    <div class="container">
	<h1>Mongo Scraper</h1>
	<p>Scrape articles from Catster and save them to read later. Write notes about it and save them to look at later too.</p>
    </div>
</div>

<div id="noArticles">
    {{#with noArt}}
    {{this.message}}
    {{/with}}
</div>

<div class="panel panel-default">
    {{#each artDb}}
    <div class="panel-body">
	<p class="title">{{this.title}}</p>
	<br/>
	<a class="link">{{this.link}}</a>
	<br/>
	<a class="btn btn-default" href="/saved">Save Article</a>
    </div>
    
    {{/each}}
</div>
